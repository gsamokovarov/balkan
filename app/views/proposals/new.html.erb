<% title "Call for Proposals" %>

<%= render "section", title: "Call for Proposals", title_element: "h1", highlight: true do %>
  <% if Current.event.speaker_applications_end_date.future? %>
    <%= render "proposals/form", proposal: @proposal %>
  <% else %>
    <%= render "proposals/closed" %>
  <% end %>
<% end %>
