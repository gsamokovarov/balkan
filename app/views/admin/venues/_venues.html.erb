<%# locals: (venues:) %>

<%= admin_table venues do
  column "#",   -> { link_to it.id, admin_venue_path(it) }
  column :name, -> { link_to it.name, admin_venue_path(it) }
  column :description,  -> { truncate it.description, length: 50 }
  column :address,  -> { truncate it.address, length: 50 }
end %>
