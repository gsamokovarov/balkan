<%= render "section", title: "Venues", cta: -> {
  admin_button(:primary, link: new_admin_venue_path) { "New venue" }
} do %>
  <% if @venues.any? %>
    <%= render "venues", venues: @venues %>
  <% else %>
    <%= render "empty_state" do %>
      <%= admin_button(:primary, link: new_admin_venue_path) { "New venue" } %>
    <% end %>
  <% end %>
<% end %>
