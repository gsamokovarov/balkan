<%# locals: (ticket_types:) %>

<%= admin_table ticket_types do
  column "#",      -> { link_to _1.id, [:admin, _1.event, _1] }
  column :name,    -> { link_to _1.name, [:admin, _1.event, _1] }
  column :price,   -> { link_to _1.price, [:admin, _1.event, _1] }
  column :enabled, -> { _1.enabled ? "Yes" : "No" }
end %>
