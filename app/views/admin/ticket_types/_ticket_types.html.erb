<%# locals: (ticket_types:) %>

<%= admin_table ticket_types do
  column "#",      -> { link_to it.id, [:admin, it.event, it] }
  column :name,    -> { link_to it.name, [:admin, it.event, it] }
  column :price,   -> { link_to it.price, [:admin, it.event, it] }
  column :enabled, -> { it.enabled ? "Yes" : "No" }
end %>
