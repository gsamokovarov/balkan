<%# locals: (embedding:) %>

<%= render "section", title: embedding.name, description: embedding.description, cta: -> {
  admin_button(:primary, link: embedding.url, target: "_blank", rel: "noopener") { "View" }
} do %>
  <iframe src="<%= embedding.url %>" class="w-full h-[620px] border-2 border-gray-300 rounded-md"></iframe>
<% end %>
