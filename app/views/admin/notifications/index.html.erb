<%= render "section", title: "Notifications", cta: -> {
  admin_button(:primary, link: new_admin_event_notification_path) { "New Notification" }
} do %>
  <% if @notifications.any? %>
    <%= render "notifications", notifications: @notifications %>
    <%= admin_paginate @notifications %>
  <% else %>
    <%= render "empty_state" do %>
      <%= admin_button(:primary, link: new_admin_event_notification_path) { "New Notification" } %>
    <% end %>
  <% end %>
<% end %>
