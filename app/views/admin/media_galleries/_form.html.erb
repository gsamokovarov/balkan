<%# locals: (media_gallery:) %>

<%= admin_form media_gallery, url: admin_event_media_gallery_path(media_gallery.event) do |f| %>
  <%= f.text_input :title %>
  <%= f.text_area_input :description %>
  <%= f.text_input :videos_url, required: true %>
  <%= f.text_input :photos_url %>
  <%= f.file_input :highlights, multiple: true %>
  <%= f.submit %>
<% end %>
